#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules/webpack-dev-server/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules/webpack-dev-server/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/webpack-dev-server@4.9.3_webpack-cli@5.0.1_webpack@5.73.0/node_modules:/mnt/c/Users/Maria/Aptugo/build/SistemaDeFacturacion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../bin/webpack-dev-server.js" "$@"
fi
